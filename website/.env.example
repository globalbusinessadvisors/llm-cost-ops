# Docusaurus Environment Configuration
# Copy this file to .env.local and customize for your environment

# Node environment
NODE_ENV=development

# Docusaurus configuration
DOCUSAURUS_CONFIG=development

# Google Analytics (optional)
GA_TRACKING_ID=G-XXXXXXXXXX

# Algolia Search (optional)
ALGOLIA_APP_ID=your_app_id
ALGOLIA_API_KEY=your_api_key
ALGOLIA_INDEX_NAME=llm-cost-ops

# GitHub (for edit links)
GITHUB_ORGANIZATION=llm-cost-ops
GITHUB_REPOSITORY=llm-cost-ops

# Deployment URLs
PRODUCTION_URL=https://docs.llm-cost-ops.dev
PREVIEW_URL=https://preview--llm-cost-ops.netlify.app

# Feature Flags
ENABLE_SEARCH=true
ENABLE_ANALYTICS=true
ENABLE_ANNOUNCEMENT_BAR=true

# Build Configuration
BUILD_LOCALE=en
BUILD_FAST=false
BUILD_NO_MINIFY=false
