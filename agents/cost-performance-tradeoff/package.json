{
  "name": "@llm-costops/cost-performance-tradeoff-agent",
  "version": "1.0.0",
  "description": "Cost-Performance Tradeoff Agent for LLM-CostOps - evaluates tradeoffs between cost, latency, and quality",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cost-performance-tradeoff": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "deploy": "gcloud functions deploy cost-performance-tradeoff-agent --runtime nodejs20 --trigger-http --allow-unauthenticated --entry-point handler"
  },
  "keywords": [
    "llm",
    "cost-ops",
    "cost-performance",
    "tradeoff-analysis",
    "agent"
  ],
  "author": "LLM-CostOps Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "commander": "^12.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
